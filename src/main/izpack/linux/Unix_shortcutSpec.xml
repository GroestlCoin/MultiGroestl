<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="MultiGroestl"
  location="applications"/>

   <shortcut
     name="MultiGroestl"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/multigroestl-exe.jar"
     initialState="normal"
     iconFile="$INSTALL_PATH/multigroestl48.png"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="MultiGroestl">
   </shortcut>
   <shortcut
     name="Uninstall MultiGroestl"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/Uninstaller/uninstaller.jar"
     initialState="normal"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="Uninstall MultiGroestl">
   </shortcut>
</shortcuts>

